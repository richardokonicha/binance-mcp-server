{
  "name": "binance-mcp-server",
  "version": "1.1.1",
  "description": "MCP server for Binance market data and trading integration",
  "main": "dist/index.js",
  "bin": {
    "binance-mcp-server": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "watch": "nodemon --exec tsx src/index.ts",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "binance",
    "mcp",
    "claude",
    "trading",
    "api"
  ],
  "author": "richardokonicha",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richardokonicha/binance-mcp-server.git"
  },
  "homepage": "https://github.com/richardokonicha/binance-mcp-server#readme",
  "bugs": {
    "url": "https://github.com/richardokonicha/binance-mcp-server/issues"
  },
  "files": [
    "dist",
    "README.md",
    "README_zh.md",
    "README_ja.md",
    "CLAUDE.md"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "binance-api-node": "^0.12.6",
    "dotenv": "^16.3.1",
    "https-proxy-agent": "^7.0.6",
    "socks-proxy-agent": "^8.0.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "nodemon": "^3.0.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
